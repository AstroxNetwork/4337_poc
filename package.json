{
  "name": "4337_poc",
  "version": "0.0.1",
  "description": "project monorepo",
  "private": true,
  "scripts": {
    "contract:compile": "hardhat compile",
    "hardhat:run": "npx hardhat --config hardhat.config.js node --network hardhat",
    "script:fixtures": "swc ./clients/fixtures -d dist",
    "script:bootstrap": "swc ./scripts -d dist",
    "project:pre": "pnpm run script:bootstrap && node ./dist/ic_deployer.js --clean",
    "project:bootstrap": "pnpm run project:clean && pnpm run project:create",
    "project:build": "node ./dist/builder.js",
    "project:clean": "node ./dist/deployer.js --clean",
    "project:create": "node ./dist/deployer.js --create",
    "project:install": "node ./dist/deployer.js --install",
    "project:reinstall": "node ./dist/deployer.js --reinstall",
    "project:upgrade": "node ./dist/deployer.js --upgrade",
    "project:post_install": "pnpm run project:post_install:infra && pnpm run project:post_install:apps",
    "project:post_install:infra": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --verbose -t infra",
    "project:post_install:apps": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --verbose -t apps",
    "project:post_install:project": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --verbose -t",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "project:script": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --verbose --testPathIgnorePatterns=/post_install/ -t",
    "format": "prettier --write {scripts,clients}/**/*.{js,ts,less,json}",
    "format:json": "prettier --write artifacts/**/*.json",
    "format:idl": "prettier --write clients/idls/**/*.ts",
    "poc:setup": "pnpm run project:pre && pnpm run project:bootstrap && pnpm run project:build --infra && pnpm project:install --infra && pnpm run project:build --project=project_wallet && pnpm run project:build --project=project_assets && pnpm run project:post_install:infra"
  },
  "author": "devs@astrox",
  "license": "ISC",
  "dependencies": {
    "@ethers-ancillary/bsc": "^0.0.3",
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/hash": "^5.7.0",
    "@ethersproject/keccak256": "^5.7.0",
    "@ethersproject/logger": "^5.7.0",
    "@ethersproject/properties": "^5.7.0",
    "@ethersproject/signing-key": "^5.7.0",
    "@ethersproject/transactions": "^5.7.0",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.141",
    "@swc/jest": "^0.2.20",
    "@types/mockjs": "^1.0.6",
    "@types/yargs": "^17.0.10",
    "bip32": "^3.0.1",
    "bip39": "^3.0.4",
    "buffer": "^6.0.3",
    "crc": "^4.1.1",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "ethers": "^5.7.0",
    "js-sha256": "^0.9.0",
    "mockjs": "^1.1.0",
    "starkbank-ecdsa": "^1.1.4",
    "tiny-secp256k1": "^2.2.1",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.18",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "hardhat": "^2.11.1",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.5.0",
    "tsc-alias": "^1.6.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}